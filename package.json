{
  "name": "backendnode",
  "version": "1.0.0",
  "description": "Simple chat",
  "main": "dist/server.js",
  "scripts": {
    "service-template": "sh ./scripts/service_skeleton.sh",
    "dev": "ts-node-dev -r tsconfig-paths/register src/server.ts",
    "start": "node build/server.js",
    "lint": "eslint",
    "migration": "ts-node-dev -r tsconfig-paths/register src/libs/umzug/index.ts",
    "migrations:generate": "npm run migration -- create --name",
    "migrations:run": "npm run migration -- up",
    "migrations:revert": "npm run migration -- down",
    "migrations:delete": "npm run migration -- down --to 0"
  },
  "keywords": [
    "chat",
    "server",
    "server",
    "node",
    "mongo"
  ],
  "devDependencies": {
    "@types/continuation-local-storage": "^3.2.4",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/express-debug": "^1.1.33",
    "@types/express-handlebars": "^6.0.0",
    "@types/joi": "^17.2.3",
    "@types/node": "^18.11.18",
    "@types/pg": "^8.6.6",
    "@types/umzug": "^2.3.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^3.4.1",
    "express-debug": "^1.0.2",
    "prettier": "^2.3.2",
    "sequelize-cli": "^6.5.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.9.4"
  },
  "author": "Enzo Retamozo",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@hapi/boom": "^10.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "joi": "^17.7.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.28.0",
    "umzug": "^3.2.1"
  }
}
